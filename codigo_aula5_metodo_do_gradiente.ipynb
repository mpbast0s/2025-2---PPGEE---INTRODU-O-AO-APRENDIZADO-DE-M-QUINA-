      "source": [
        "# Carregando nosso conjunto de dados:\n",
        "x_trein = np.array([ 0.2,2.,5.,10.])       # característica\n",
        "y_trein = np.array([ 10.64975337,106.49753367,266.24383418,532.48766835])    # valor alvo"
